{
    "name": "aksoyih/better-dump",
    "description": "A beautiful debugging tool for PHP applications with syntax highlighting and collapsible output",
    "type": "library",
    "version": "1.0.2",
    "keywords": ["debug", "dump", "var-dump", "dd", "pretty-dump"],
    "license": "MIT",
    "authors": [
        {
            "name": "Haluk Aksoy",
            "email": "me@halukaksoy.dev"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Aksoyih\\BetterDump\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}